            <ComboBoxItem Content="Jasionka (Rzeszów)"/>
            <ComboBoxItem Content="Goleniów (Szczecin)"/>
            <ComboBoxItem Content="Świdnik (Lublin)"/>
            <ComboBoxItem Content="Szwederowo (Bydgoszcz)"/>
